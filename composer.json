{
    "name": "oyale/pwpush-php",
    "description": "A PHP wrapper to easily push passwords to pwpush.com",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "justinrainbow/json-schema": "^5.2"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.4",
        "filp/whoops": "^2.14",
        "phpunit/phpunit": "^7"
    },
    "license": "LGPL-3.0-or-later",
    "autoload": {
        "psr-4": {
            "PwPush\\": "src/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
